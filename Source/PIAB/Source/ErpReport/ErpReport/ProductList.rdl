  <DataSets>
    <DataSet Name="ProductListDSet">
      <Query>
        <DataSourceName>ProductListDS</DataSourceName>
        <CommandText>select (select Name from Erp.ProductCategory where ProductCategoryId=Erp.Product.ProductCategoryId) as ProductCategory,
(select Name from Erp.ProductSubCategory where ProductSubCategoryId=Erp.Product.ProductSubCategoryId) as  ProductSubCategory,
 ProductName,ShortName,UnitPrice,PackSize
from Erp.Product
order By ProductCategory,ProductSubCategory,ProductName
</CommandText>
      </Query>
