    <DataSet Name="ProductFormulaDSet">
      <Query>
        <DataSourceName>ProductFormulaDS</DataSourceName>
        <CommandText>select (select ProductName from Erp.Product where ProductId=Erp.ProductFormula.FProductId)+' ('+ cast((select FormulaQty from Erp.Product where ProductId=Erp.ProductFormula.FProductId) as varchar(100))+' Kg)' as Product,
(select ProductName from Erp.Product where ProductId=Erp.ProductFormula.RProductId) as Rawmaterial,
Qty

from Erp.ProductFormula</CommandText>
      </Query>
